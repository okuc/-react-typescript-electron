{
  "name": "react-typescript-electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.37",
    "@types/react": "^16.9.34",
    "@types/react-document-title": "^2.0.4",
    "@types/react-dom": "^16.9.7",
    "antd": "^4.2.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-electron-contextmenu": "^1.0.0",
    "react-scripts": "3.4.1",
    "terminal-in-react": "^4.3.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "start-electron": "cross-env NODE_ENV=development electron . --debug",
    "start-electron-prod": "electron .",
    "build-electron": "electron-builder",
    "build-electron-green": "electron-builder --dir"
  },
  "build": {
    "appId": "com.okuc.test",
    "productName": "react-typescript-electron",
    "extends": null,
    "directories": {
      "output": "build-electron"
    },
    "files": [
      "./build/**/*",
      "./main.js",
      "./package.json"
    ],
    "win": {
      "icon": "src/asset/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "devtron": "^1.4.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "react-app-rewire-multiple-entry": "^2.2.0",
    "react-app-rewired": "^2.1.6"
  }
}
